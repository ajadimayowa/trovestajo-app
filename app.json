{
  "expo": {
    "name": "TOV Minds Alajo",
    "slug": "trovest-ajo",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Tro-Vest Ajo will like to gain access to your photos to upload.",
          "cameraPermission": "Allow Tro-Vest Ajo to access your camera"
        }
      ]
    ],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "stretch",
      "background": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.trovestfinance.mobile"
    },
    "web": {
      "favicon": "./assets/adaptive-icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b40aeb25-7ab5-464e-bfe4-0a212f751648"
      }
    }
  }
}
